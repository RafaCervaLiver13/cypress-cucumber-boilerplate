{
  "json": {
    "enabled": true,
    "output": "cypress/reports/cucumber-report.json",
    "formatter": "node",
    "args": [
      "./node_modules/@deepakvishwakarma/cucumber-json-formatter/lib/main.js"
    ]
  },
  "messages": {
    "enabled": true,
    "output": "cypress/reports/cucumber-messages.ndjson"
  },
  "stepDefinitions": [
    "cypress/e2e/[filepath]/**/*.{js,ts}",
    "cypress/e2e/[filepath].js",
    "cypress/support/step_definitions/**/*.{js,ts}",
    "cypress/e2e/common-step-definitions/*.js"
  ],
  "filterSpecs": true,
  "omitFiltered": true
}